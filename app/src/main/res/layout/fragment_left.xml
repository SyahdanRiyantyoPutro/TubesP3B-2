<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/abuIF">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_pengumuman"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/pengumuman_title"
            android:backgroundTint="@color/biruIF"
            android:textColor="@color/birutuaIF"/>

        <Button
            android:id="@+id/btn_pertemuan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/pertemuan_title"
            android:backgroundTint="@color/biruIF"
            android:textColor="@color/birutuaIF"/>

        <Button
            android:id="@+id/btn_pramatkul"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/pramatkul_title"
            android:backgroundTint="@color/biruIF"
            android:textColor="@color/birutuaIF"/>

        <Button
            android:id="@+id/btn_pengaturan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/pengaturan_title"
            android:backgroundTint="@color/biruIF"
            android:textColor="@color/birutuaIF"/>

        <Button
            android:id="@+id/btn_signout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/signout_title"
            android:backgroundTint="@color/biruIF"
            android:textColor="@color/birutuaIF"/>

    </LinearLayout>
</FrameLayout>